{
    "manifest_version": 2,
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "version": "0.6",
    "default_locale": "en",
    "browser_specific_settings": {
        "gecko": {
            "id": "hidao80@github__comShare-with-ShareGPT"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://chat.openai.com/*",
                "*://chat.openai.com/c/*"
            ],
            "js": ["app.js"]
        }
    ],
    "icons": {
        "256": "Identicon.png"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "*://sharegpt.com/api/*"
    ]
}
